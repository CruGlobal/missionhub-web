{
    "name": "campus-contacts",
    "version": "1.0.0",
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "gql:schema": "apollo client:download-schema",
        "gql:codegen": "apollo client:codegen --target typescript --includes={src,pages}/**/*.{ts,tsx}",
        "test:cypress": "cypress run",
        "test:cypress:open": "run cypress open",
        "test:cypress:ci": "DEBUG=start-server-and-test START_SERVER_AND_TEST_INSECURE=0 start-server-and-test start https-get://localhost:8080 test:cypress",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "test:snapshot": "jest -u",
        "test:changes": "jest -o",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
        "lint:ts": "tsc",
        "prettier:check": "prettier '{{src,pages,bin,test,__{tests,mocks}__}/**/*.{js,json,html},./*.{js,json,html,yml}}' --list-different",
        "prettier:write": "prettier '{{src,pages,bin,test,__{tests,mocks}__}/**/*.{js,json,html},./*.{js,json,html,yml}}' --write",
        "precommit": "pretty-quick --staged",
        "onesky:upload": "babel-node ./bin/oneskyUpload.js",
        "onesky:download": "babel-node ./bin/oneskyDownload.js",
        "postinstall": "patch-package"
    },
    "dependencies": {
        "@apollo/client": "^3.2.2",
        "@emotion/core": "^10.0.35",
        "@emotion/styled": "^10.0.27",
        "@nivo/bar": "^0.61.1",
        "d3-shape": "^1.3.7",
        "emotion-theming": "^10.0.27",
        "graphql": "^15.3.0",
        "graphql-tag": "^2.11.0",
        "i18next": "^19.4.5",
        "next": "^9.5.3",
        "next-auth": "^3.1.0",
        "next-compose-plugins": "^2.2.0",
        "next-optimized-images": "^3.0.0-canary.10",
        "patch-package": "^6.2.2",
        "postinstall-postinstall": "^2.1.0",
        "react": "^16.13.1",
        "react-dates": "^21.8.0",
        "react-dom": "^16.13.1",
        "react-i18next": "^11.5.0",
        "react-redux": "^5.1.2",
        "rollbar": "^2.16.2"
    },
    "devDependencies": {
        "@babel/node": "^7.10.1",
        "@brainly/onesky-utils": "^1.4.0",
        "@testing-library/react": "^10.0.4",
        "@types/d3-shape": "^1.3.2",
        "@types/faker": "^4.1.12",
        "@types/graphql": "^14.5.0",
        "@types/jest": "^25.2.3",
        "@types/next-auth": "^3.1.10",
        "@types/react": "^16.9.35",
        "@types/react-dates": "^17.1.10",
        "@types/react-dom": "^16.9.8",
        "@types/react-redux": "^7.1.9",
        "@types/react-router-dom": "^5.1.5",
        "@typescript-eslint/parser": "^3.0.2",
        "apollo": "^2.31.0",
        "babel-eslint": "^10.1.0",
        "babel-jest": "^26.0.1",
        "babel-plugin-emotion": "^10.0.33",
        "cypress": "^4.7.0",
        "dotenv": "^8.2.0",
        "eslint": "^7.1.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-loader": "^4.0.2",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-prettier": "^3.1.3",
        "faker": "^4.1.0",
        "fraql": "^1.2.1",
        "graphql-tools": "^6.2.4",
        "husky": "^4.2.5",
        "jest": "^26.0.1",
        "jest-css-modules": "^2.1.0",
        "jest-emotion": "^10.0.32",
        "jest-svg-transformer": "^1.0.0",
        "prettier": "^2.0.5",
        "pretty-quick": "^2.0.1",
        "start-server-and-test": "^1.11.0",
        "ts-jest": "^26.1.0",
        "typescript": "^3.9.3"
    }
}
